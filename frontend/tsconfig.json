{
  "compilerOptions": {
    "target": "ES2022",
    "useDefineForClassFields": false,
    "module": "ES2022",
    "lib": ["ES2022", "DOM"],
    "declaration": false,
    "sourceMap": true,
    "outDir": "./dist/out-tsc",
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "emitDecoratorMetadata": false,
    "esModuleInterop": true,
    "typeRoots": ["node_modules/@types"]
  }
}
